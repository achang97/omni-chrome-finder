{
  "version": "0.0.0",
  "name": "Omni",
  "manifest_version": 2,
  "description": "A lightweight Q&A tool that helps you document and find information faster.",
  "browser_action": {
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "web_accessible_resources": [
    "inject.html"
  ],
  "background": {
    "page": "background.html"
  },
  "permissions": ["activeTab", "storage"],
  "content_security_policy": "default-src 'self'; script-src 'self'; style-src * 'unsafe-inline'; img-src 'self' data:;"
}
